{
  "name": "@internetcamera/contracts",
  "version": "0.0.60",
  "main": "dist/typechain/index.js",
  "types": "dist/typechain/index.d.ts",
  "files": [
    "dist/**/*",
    ".dist/*"
  ],
  "scripts": {
    "deploy-local": "ts-node scripts/deploy.ts --chainId 1337 && cp -R addresses dist",
    "deploy-mumbai": "ts-node scripts/deploy.ts --chainId 80001 && cp -R addresses dist",
    "prebuild": "patch-package",
    "build": "yarn build:contracts && yarn build:package",
    "prepublishOnly": "yarn build",
    "test": "hardhat test",
    "compile": "hardhat clean && hardhat compile",
    "typechain": "rm -rf ./typechain && typechain --show-stack-traces --target ethers-v5 --out-dir ./typechain 'artifacts/!(build-info)/**/!(*.dbg*)*.json'",
    "build:contracts": "yarn compile && yarn typechain",
    "build:package": "rm -rf ./dist && tsc && cp typechain/*.d.ts dist/typechain && cp -R addresses dist && cp -R artifacts/contracts dist/artifacts && cp -R contracts dist",
    "chain": "ganache-cli -h 0.0.0.0 --networkId 1337 --accounts 20 -e 10000000000 -l 10000000 -m 'nominee arrange pistol aim book head mind anxiety lobster system forest dose void marine eyebrow'"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.1.7",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.1.2",
    "@types/chai": "^4.2.17",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^10.0.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.4.4",
    "ganache-cli": "^6.12.2",
    "hardhat": "^2.4.1",
    "hardhat-deploy": "^0.7.5",
    "hardhat-typechain": "^0.3.5",
    "minimist": "^1.2.5",
    "patch-package": "^6.4.7",
    "ts-node": "^9.1.1",
    "typechain": "^5.1.1",
    "typescript": "^4.2.4"
  },
  "gitHead": "38c3d4abd4a012c555362c20dacb367bd7c44ffc"
}
